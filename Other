Title = "KOMAT Hub",
Desc = "Evade Hub on top",
Icon = 105059922903197,
Theme = "Dark",
Config = { Keybind = Enum.KeyCode.LeftControl, Size = UDim2.new(0,500,0,400) },
CloseUIButton = { Enabled=true, Text="Evade Hub" }
})

-- Sidebar line
