    Callback = function()
        local url = "https://pastebin.com/raw/DSZCMGqh" -- ‡πÉ‡∏ä‡πâ raw link ‡∏Ç‡∏≠‡∏á Pastebin
        local success, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)
        if not success then
            warn("‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Emote ‡πÑ‡∏î‡πâ: "..tostring(err))
        end
